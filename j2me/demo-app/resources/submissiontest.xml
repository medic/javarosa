<h:html xmlns="http://www.w3.org/2002/xforms"
        xmlns:h="http://www.w3.org/1999/xhtml"
        xmlns:ev="http://www.w3.org/2001/xml-events"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:jr="http://openrosa.org/javarosa">
  <h:head>
    <h:title>Submission Element Test</h:title>

    <model>
    
      <instance>
        <data xmlns="http://openrosa.org/testforms/submission" version="1" uiVersion="1">
        	<first>
        		<a/>
        		<b>stuff</b>
        	</first>
        	<second/>
        	<third>stuff</third>
        </data>
      </instance>

      <bind id="firstnode" nodeset="/data/first" />

	  <submission bind="firstnode" method="post" action="http://staging.commcarehq.org/receiver/submit/Demo"/>  

    </model>
  </h:head>
  <h:body>
  
  <input ref="/data/first/a">
  	<label>Input Value</label>
   </input>
   <input ref="/data/second">
  	<label>Input Value</label>
   </input>
  
  </h:body>
</h:html>